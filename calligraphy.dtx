% \begin{macro}{\spath@trimfirst}
%    \begin{macrocode}
\def\spath@trimfirst#1#2#3#4\relax{%
  \edef\this@action{\string#1}%
  \ifx\this@action\spath@moveto
  \def\spath@trimmed@path{#4}%
  \else
  \def\spath@trimmed@path{#1{#2}{#3}#4}%
  \fi
}
%    \end{macrocode}
% \end{macro}
