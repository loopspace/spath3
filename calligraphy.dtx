    \else
    \ifx\spath@this@rlen@action\spath@curveto
     \stepcounter{spath@length}%
    \fi
    \fi
    \edef\spath@to@add{}%
  }
  \let\spath@rlen@append=\ge@addto@macro
  \def\@temp{\spath@walk\spath@rlen@path\spath@rlen@action\spath@rlen@append}
  \expandafter\@temp#1\relax
}
